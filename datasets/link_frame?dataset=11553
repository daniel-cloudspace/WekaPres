    //function to fix height of iframe!
    var calcHeight = function() {
      var headerHeight = $('#top-bar').height();
      $('#link-frame').height($(window).height() - headerHeight);
    }

    $(document).ready(function() {
      calcHeight();
      $('#header a.close').mouseover(function() {
        $('#top-bar a.close').addClass('activated');
      }).mouseout(function() {
        $('#top-bar a.close').removeClass('activated');
      });
    });

    $(window).resize(function() {
      calcHeight();
    }).load(function() {
      calcHeight();
    });
  </script>

